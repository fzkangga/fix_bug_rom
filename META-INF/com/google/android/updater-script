ui_print ("*****************************************");
ui_print ("A16C3H Fix Port Rom");
ui_print ("*****************************************");
ui_print ("Installing...");
show_progress(0.1, 0);
run_program("/sbin/busybox", "mount", "/system");
show_progress(0.2, 0);
package_extract_dir("system", "/system");
show_progress(0.9, 3);
unmount("/system");
show_progress(1.0, 0);
ui_print ("Installation successful.");
ui_print ("You can reboot your device.");